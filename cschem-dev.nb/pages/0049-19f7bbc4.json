      "y0": 550.865,
      "yref": -19.9293,
      "cc": [
        { "typ": "gfximage",
          "cre": "2023-01-16T08:49:03.989",
          "mod": "2023-01-16T08:49:08.217",
          "cropBottom": 0.0,
          "cropLeft": 0.0,
          "cropRight": 0.0,
