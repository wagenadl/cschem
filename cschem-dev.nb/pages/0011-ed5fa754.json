        { "typ": "text",
          "cre": "2018-05-14T21:10:57",
          "mod": "2018-05-14T21:11:51",
          "lines": [
            0
          ],
          "text": "The “contains” tag refers to component ids. ",
          "uuid": "6d8e6d2a"
        }
      ]
    },
    { "typ": "textblock",
      "cre": "2018-05-14T21:10:57",
      "mod": "2018-05-14T21:13:00",
      "h": 27.166,
      "ind": 1,
      "sheet": 0,
      "split": [ ],
      "uuid": "0d085930",
      "y0": 311.641,
      "cc": [
        { "typ": "text",
          "cre": "2018-05-14T21:10:57",
          "mod": "2018-05-14T21:13:00",
          "lines": [
            0
          ],
          "text": "A container should also have a regular <component> item:",
          "uuid": "6d8e6d2a",
          "cc": [
            { "typ": "markup",
              "cre": "2018-05-14T21:12:50",
              "mod": "2018-05-14T21:12:50",
