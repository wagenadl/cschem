      "cc": [
        { "typ": "text",
          "cre": "2018-05-14T21:29:12",
          "mod": "2018-05-14T21:29:54",
          "lines": [
            0,
            56
          ],
          "text": "The presence of a container item should cause contained components to vanish from the parts list.",
          "uuid": "e9add6d1",
          "cc": [
            { "typ": "markup",
              "cre": "2018-05-14T21:29:36",
              "mod": "2018-05-14T21:29:36",
