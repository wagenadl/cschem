      "y0": 601.652,
      "cc": [
        { "typ": "text",
          "cre": "2018-06-19T18:42:08",
          "mod": "2018-06-19T18:43:54",
          "lines": [
            0
          ],
          "text": "ExpressPCB has only one DC connector, CP202-A on Digikey:",
          "uuid": "a91e2e8b"
        }
      ]
    },
